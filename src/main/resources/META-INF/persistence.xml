<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FDMSPU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/fdms</jta-data-source>
    <class>com.arslorem.fdms.entities.Beneficiary</class>
    <class>com.arslorem.fdms.entities.IdType</class>
    <class>com.arslorem.fdms.entities.District</class>
    <class>com.arslorem.fdms.entities.City</class>
    <class>com.arslorem.fdms.entities.Distribution</class>
    <class>com.arslorem.fdms.entities.UOM</class>
    <class>com.arslorem.fdms.entities.Basket</class>
    <class>com.arslorem.fdms.entities.BasketItem</class>
    <class>com.arslorem.fdms.entities.MaterialIssue</class>
    <class>com.arslorem.fdms.entities.MaterialReceipt</class>
    <class>com.arslorem.fdms.entities.MaterialIssueItem</class>
    <class>com.arslorem.fdms.entities.MaterialReceiptItem</class>
    <class>com.arslorem.fdms.entities.BasketVoucher</class>
    <class>com.arslorem.fdms.entities.Warehouse</class>
    <class>com.arslorem.fdms.entities.Item</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
      <property name="eclipselink.target-database" value="MySQL"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
